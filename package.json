{
  "name": "openapi",
  "version": "0.0.1",
  "description": "Node app for 6apps",
  "keywords": [
    "backend"
  ],
  "scripts": {
    "k6": "postman-to-k6 collection.json -o k6-script.js",
    "mock": "prism mock https://raw.githack.com/OAI/OpenAPI-Specification/master/examples/v3.0/petstore-expanded.yaml",
    "mock-server": "prism mock ./src/openapi.yaml",
    "generate": "npx @redocly/cli build-docs src/openapi.yaml",
    "preview": "redocly preview-docs src/openapi.yaml",
    "collection": "openapi2postmanv2 -s src/openapi.yaml -o collection.json",
    "client": "autorest --input-file=src/swagger.yaml --typescript --output-folder=client/ --namespace=crm",
    "client:new": "autorest --typescript src/swagger.yaml"
  },
  "license": "MIT",
  "dependencies": {
    "autorest": "^3.7.1",
    "path": "^0.12.7",
    "swagger-ui-express": "^5.0.0"
  },
  "devDependencies": {
    "@apideck/postman-to-k6": "^1.8.10",
    "@redocly/cli": "^1.9.1",
    "@stoplight/prism-cli": "^5.5.4",
    "@types/swagger-jsdoc": "^6.0.4",
    "@types/swagger-ui-express": "^4.1.6",
    "openapi-to-postmanv2": "^4.20.0",
    "rimraf": "^5.0.1"
  }
}
